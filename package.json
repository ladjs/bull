{
  "name": "@ladjs/bull",
  "description": "Bull for Lad",
  "version": "1.0.7",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "ava": {
    "failFast": true,
    "verbose": true,
    "files": [
      "test/**/*",
      "!test/helpers"
    ]
  },
  "bugs": {
    "url": "https://github.com/ladjs/bull/issues",
    "email": "niftylettuce@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
    "Shaun Warman <shaunwarman1@gmail.com> (https://shaunwarman.com)"
  ],
  "dependencies": {
    "@ladjs/redis": "^1.0.4",
    "@ladjs/shared-config": "^3.0.8",
    "auto-bind": "^4.0.0",
    "bull": "^3.14.0",
    "is-string-and-not-blank": "^0.0.2",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "ava": "^3.9.0",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.2",
    "delay": "^4.3.0",
    "eslint": "6.8.0",
    "eslint-config-xo-lass": "^1.0.3",
    "eslint-plugin-node": "^11.1.0",
    "fixpack": "^3.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "ms": "^2.1.2",
    "nyc": "^15.1.0",
    "remark-cli": "^8.0.0",
    "remark-preset-github": "^1.0.1",
    "xo": "0.25"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/ladjs/bull",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "agenda",
    "bull",
    "cron",
    "cronjob",
    "crontab",
    "express",
    "frequency",
    "frequent",
    "graceful",
    "human",
    "interval",
    "job",
    "jobs",
    "koa",
    "lad",
    "lass",
    "moment",
    "mongo",
    "mongodb",
    "mongoose",
    "recur",
    "recurred",
    "recurring",
    "redis",
    "reload",
    "scheduler",
    "scheduling",
    "setup"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "publishConfig": {
    "access": "public"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/bull"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo",
    "test": "npm run lint && npm run test-coverage",
    "test-coverage": "cross-env NODE_ENV=test nyc ava"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
